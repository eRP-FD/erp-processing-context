    <subject>
        <identifier>
            <system value="http://fhir.de/sid/gkv/kvid-10" />
            <value value="##KVNR##" />
        </identifier>
    </subject>
    <performer>
        <actor>
            <identifier>
                <system value="https://gematik.de/fhir/sid/telematik-id" />
                <value value="##TELEMATIK_ID##" />
            </identifier>
        </actor>
