      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Identifier.LANR.system">
    <xs:complexContent>
      <xs:extension base="Element">
        <xs:attribute name="value" type="uri-primitive" fixed="https://fhir.kbv.de/NamingSystem/KBV_NS_Base_ANR"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
</xs:schema>